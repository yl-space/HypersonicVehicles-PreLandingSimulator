    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "three": "^0.158.0",
    "csvtojson": "^2.0.10"
  },
  "devDependencies": {
    "vite": "^4.4.9",
